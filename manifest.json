{
  "manifest_version": 3,
  "name": "Dark Mode Injector",
  "version": "1.1",
  "description": "Toggle dark mode on any website.",
  "permissions": ["storage"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
      "http://*/*",
      "https://*/*",
      "file://*/*"
    ],
      "js": ["content.js"]
    }
  ]
}
